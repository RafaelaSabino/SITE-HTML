    <title>Document</title>
    <script>
        $quantidade =  prompt("Quantas peças possui no estoque: ")
        $resultado = $quantidade

        if($quantidade < 50) 
        {
          $resultado = ' BAIXO '
        }
        else 
        
          if($quantidade < 100)
        { 
            $resultado = ' NORMAL '
        }
        else 
        {
            $resultado = ' IDOSO'
        }
                document.write(" <center> <font color='blue' size='7'> ESTOQUE </font>")
                document.write("<table border='1' width='600'>")
                document.write("<tr bgcolor='#836FFF' align='center'>")
                document.write(" <td width='350' align='center'><B>QUANTIDADE</B></td>")
                document.write(" <td width='80' align='center'><B>CLASSIFICAÇÃO</B></td>")
                document.write(" </tr>")
                document.write(" <tr>")
                document.write("<td>")
                document.write($quantidade)
                document.write("</td>")
                document.write("<td>")
                document.write($resultado)
                document.write("</td>")
                document.write(" </tr>")
                document.write("</table>")

    </script>
